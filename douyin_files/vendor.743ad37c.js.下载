/***/ "682c223c8090c9c699fb":
/***/ (function(module, exports, __webpack_require__) {

/* WEBPACK VAR INJECTION */(function(module) {var __WEBPACK_AMD_DEFINE_FACTORY__, __WEBPACK_AMD_DEFINE_RESULT__;(function (factory) {

    if ( true && module.hasOwnProperty("exports") && typeof exports === 'object') {
        module.exports = factory();
    } else if (true) {
        !(__WEBPACK_AMD_DEFINE_FACTORY__ = (factory),
				__WEBPACK_AMD_DEFINE_RESULT__ = (typeof __WEBPACK_AMD_DEFINE_FACTORY__ === 'function' ?
				(__WEBPACK_AMD_DEFINE_FACTORY__.call(exports, __webpack_require__, exports, module)) :
				__WEBPACK_AMD_DEFINE_FACTORY__),
				__WEBPACK_AMD_DEFINE_RESULT__ !== undefined && (module.exports = __WEBPACK_AMD_DEFINE_RESULT__));
    } else {}

})(function () {
    var JSON = window.JSON || (function () //兼容IE
    {

        function parse(data) {
            return (new Function("return (" + data + ")"))();
        }

        var stringify = (function () {
            /**
             * 字符串处理时需要转义的字符表
             * @private
             */
            var escapeMap = {
                "\b": '\\b',
                "\t": '\\t',
                "\n": '\\n',
                "\f": '\\f',
                "\r": '\\r',
                '"': '\\"',
                "\\": '\\\\'
            };

            /**
             * 字符串序列化
             * @private
             */
            function encodeString(source) {
                if (/["\\\x00-\x1f]/.test(source)) {
                    source = source.replace(
                        /["\\\x00-\x1f]/g,
                        function (match) {
                            var c = escapeMap[match];
                            if (c) {
                                return c;
                            }
                            c = match.charCodeAt();
                            return "\\u00"
                                + Math.floor(c / 16).toString(16)
                                + (c % 16).toString(16);
                        });
                }
                return '"' + source + '"';
            }

            /**
             * 数组序列化
             * @private
             */
            function encodeArray(source) {
                var result = ["["],
                    l = source.length,
                    preComma, i, item;

                for (i = 0; i < l; i++) {
                    item = source[i];

                    switch (typeof item) {
                        case "undefined":
                        case "function":
                        case "unknown":
                            break;
                        default:
                            if (preComma) {
                                result.push(',');
                            }
                            result.push(JSON.stringify(item));
                            preComma = 1;
                    }
                }
                result.push("]");
                return result.join("");
            }

            /**
             * 处理日期序列化时的补零
             * @private
             */
            function pad(source) {
                return source < 10 ? '0' + source : source;
            }

            /**
             * 日期序列化
             * @private
             */
            function encodeDate(source) {
                return '"' + source.getFullYear() + "-"
                    + pad(source.getMonth() + 1) + "-"
                    + pad(source.getTodayDate()) + "T"
                    + pad(source.getHours()) + ":"
                    + pad(source.getMinutes()) + ":"
                    + pad(source.getSeconds()) + '"';
            }

            return function (value) {
                switch (typeof value) {
                    case 'undefined':
                        return 'undefined';

                    case 'number':
                        return isFinite(value) ? String(value) : "null";

                    case 'string':
                        return encodeString(value);

                    case 'boolean':
                        return String(value);

                    default:
                        if (value === null) {
                            return 'null';
                        } else if (value instanceof Array) {
                            return encodeArray(value);
                        } else if (value instanceof Date) {
                            return encodeDate(value);
                        } else {
                            var result = ['{'],
                                encode = JSON.stringify,
                                preComma,
                                item;

                            for (var key in value) {
                                if (Object.prototype.hasOwnProperty.call(value, key)) {
                                    item = value[key];
                                    switch (typeof item) {
                                        case 'undefined':
                                        case 'unknown':
                                        case 'function':
                                            break;
                                        default:
                                            if (preComma) {
                                                result.push(',');
                                            }
                                            preComma = 1;
                                            result.push(encode(key) + ':' + encode(item));
                                    }
                                }
                            }
                            result.push('}');
                            return result.join('');
                        }
                }
            };
        })();

        return { parse: parse, stringify: stringify };

    })();

    //常量配置
    var isDisableHttps = false;
    var isIntranet = true;
    var outServerName = 'cn';

    var xhrPath = 'v1/json';
    var imgPath = 'v1/gif';
    //内网地址（公司内）
    var XHR_SERVER_URL = 'https://mcs.byted.org/' + xhrPath;
    var IMG_SERVER_URL = 'https://mcs.byted.org/' + imgPath;
    //外网地址
    // 美东阿里云：vaali-mcs.byteoversea.com
    // 新加坡阿里云：sgali-mcs.byteoversea.com
    // 国内：https://mcs.snssdk.com/
    var OUT_SERVER_DOMAIN = {
        va: 'https://vaali-mcs.byteoversea.com/',
        sg: 'https://sgali-mcs.byteoversea.com/',
        cn: 'https://mcs.snssdk.com/',
    }

    var XHR_SERVER_URL_OUT = 'https://mcs.snssdk.com/' + xhrPath;
    var IMG_SERVER_URL_OUT = 'https://mcs.snssdk.com/' + imgPath;
    //测试服务器地址。只支持http。
    var XHR_SERVER_URL_DEV = 'http://10.8.131.217:9908/' + xhrPath;
    var IMG_SERVER_URL_DEV = 'http://10.8.131.217:9908/' + imgPath;

    //储存公共信息
    var defaultPostData = {
        user: {},
        header: {},
        events: [],
        verbose: 0,//1为开启debug模式
        caller: ''
    };
    var eventCommonParams = {};

    //一些必备的key。若未赋值时，则event会排队。直到这些key齐全后，才真正发送请求。
    var requiredKeys = {
        user: ['user_unique_id'],
        header: ['app_id'],
        eventCommonParams: [],
        customHeaders: [],
    };
    var isRequiredKeyReady = false;
    var isDebugMod = false;

    var eventQueue = [];//待发送队列

    /**
     * 设置event的user字段。
     * @param {object} objValue
     * @param {boolean} isMerge
     */
    function setUser(objValue, isMerge) {
        setKeyValue(defaultPostData, 'user', objValue, isMerge)
    }

    /**
     * 设置event的header字段。
     * @param {object} objValue
     * @param {boolean} isMerge
     */
    function setHeader(objValue, isMerge) {
        setKeyValue(defaultPostData, 'header', objValue, isMerge)
    }

    /**
     * 设置header.app_id的快捷方法
     * @param {number|string} value
     */
    function setAppId(value) {
        defaultPostData.header.app_id = value
    }

    /**
     * 设置header.headers的快捷方法
     * @param {object} objValue
     * @param {boolean} isMerge
     */
    function setHeaderHeaders(objValue, isMerge) {
        setKeyValue(defaultPostData.header, 'headers', objValue, isMerge)
    }

    /**
     * 设置header.headers.custom的快捷方法
     * {key:1} => {headers:{custom:{key:1}}}
     *
     * @param {object} objValue
     * @param {boolean} isMerge
     */
    function setCustomHeader(objValue, isMerge) {
        defaultPostData.header.headers = defaultPostData.header.headers || {};
        setKeyValue(defaultPostData.header.headers, 'custom', objValue, isMerge)
    }

    /**
     * 设置每个event的params都会有的键值对
     * @param {object} objValue
     */
    function setEventCommonParams(objValue) {
        objectAssign(eventCommonParams, objValue)
    }

    /**
     * 设置必备的字段。
     * 因为有些事件打点时，这些字段可能是异步配置的，导致还未有值。
     * 所以，只有当这些字段齐全的时候，一个事件才可能拼成完整的约定结构。然后才会真正发送事件。
     * @param {object} objValue
     */
    function setRequiredKeys(objValue) {
        objectAssign(requiredKeys, objValue)
    }

    /**
     * 工具方法。设置一个对象的属性，且该属性的值为一个对象。
     * @param {object} obj
     * @param {string} key
     * @param {object} objValue
     * @param {boolean} isMerge 是否替换该属性的值为新值。否则，为新增值对象的属性。
     */
    function setKeyValue(obj, key, objValue, isMerge) {
        if (typeof objValue !== 'object') {
            throw 'objValue 必须是个对象。'
        }
        isMerge = typeof isMerge === 'undefined' ? true : isMerge;
        if (isMerge) {
            if (!obj[key]) {
                obj[key] = {}
            }
            objectAssign(obj[key], objValue)
        } else {
            obj[key] = objValue
        }
    }

    /**
     * 是否禁用https，改为http协议。
     * @param {boolean} disabled
     */
    function disableHttps(disabled) {
        isDisableHttps = disabled !== false;
    }

    /**
     * 设置是否为外网服务器地址。
     * 有在公司网外使用的场景。
     * @param {boolean} isIntra
     * @param serverLocation
     */
    function setIntranetMode(isIntra, serverLocation) {
        isIntranet = isIntra
        serverLocation = typeof serverLocation === 'undefined' ? 'cn' : serverLocation
        if (!OUT_SERVER_DOMAIN[serverLocation]) {
            console.error('错误的服务器地址名称：' + serverLocation)
            setDebug(true)
        } else {
            outServerName = serverLocation
        }
    }

    /**
     * 开启或关闭测试模式。
     * @param mode
     */
    function setDebug(mode) {
        mode = typeof mode === 'undefined' ? false : mode;
        isDebugMod = mode;
        defaultPostData.verbose = mode ? 1 : 0
    }

    /**
     * 设置caller。
     * p.s.m, 如忽略，则默认为data.dataplatform.mcs
     * @param caller
     */
    function setCaller(caller) {
        defaultPostData.caller = caller
    }

    /**
     * 发送事件（打点）。
     * @param {Array|String} event 事件名或二维数组[[event,params]]
     * @param {Object} [params] 事件的params
     */
    function collect(event, params) {

        //整理成二维数组格式
        if (!isArray(event)) {
            if (params === undefined) {
                params = {}
            }
            if (typeof params !== 'object') {
                throw ('params的类型不正确！')
            }
            event = [[event, params]]
        }


        //将事件数组拼成事件对象。
        var eventData = [];
        var i;
        for (i = 0; i < event.length; i++) {

            //params可能为空。
            var newParams = event[i][1] || {};
            var finalParams = {}
            objectAssign(finalParams, eventCommonParams, newParams);

            eventData.push({
                event: event[i][0],
                params: finalParams,
            })
        }

        //推送到待发送队列、或直接发送。
        if (isRequiredKeyReady) {
            _collect_events(defaultPostData.user, defaultPostData.header, eventData)
        } else {
            for (i = 0; i < eventData.length; i++) {
                eventQueue.push(eventData[i])
            }
        }

    }

    /**
     * 真正发送数据给后端的函数。
     * @param user
     * @param header
     * @param events
     * @private
     */
    function _collect_events(user, header, events) {
        if (!isArray(events)) {
            events = [events]
        }

        //序列化一些字段
        for (var i = 0; i < events.length; i++) {
            events[i].params = JSON.stringify(events[i].params)
        }
        var headersString;
        if (header.headers) {
            headersString = JSON.stringify(header.headers);
        }

        //！不能修改原数据
        var newHeader = {};
        objectAssign(newHeader, header);
        newHeader.headers = headersString;

        var logEvent = {
            user: user,
            header: newHeader,
            events: events
        };
        if (defaultPostData.verbose === 1) {
            logEvent.verbose = 1
        }
        if (defaultPostData.caller) {
            logEvent.caller = defaultPostData.caller
        }


        sendToServer(logEvent)
    }

    /**
     * 判断必备属性是否都已经拿到值了。
     * @return {boolean}
     */
    function checkIsRequiredKeyValuesReady() {
        var lackCount = 0;
        for (var level1Key in requiredKeys) {
            if (requiredKeys.hasOwnProperty(level1Key)) {
                var keysArray = requiredKeys[level1Key];
                if (keysArray.length > 0) {
                    for (var i = 0; i < keysArray.length; i++) {
                        var curKey = keysArray[i];
                        var store = level1Key === "eventCommonParams" ? eventCommonParams : defaultPostData[level1Key];
                        if (store) {
                            if (typeof store[curKey] === 'undefined' || store[curKey] === '') {
                                lackCount++
                            }
                        }
                    }
                }
            }
        }
        return lackCount === 0
    }

    /**
     * 轮训判断，直到必备字段均齐全才发送。
     * 且一旦发送之后，该函数应该不会再被执行。
     * ！假定一旦必备字段齐全，则不会再缺失。
     *
     */
    function execQueue() {
        var queueTimer = null;

        var doExeQueue = function () {
            queueTimer && clearTimeout(queueTimer);
            if (checkIsRequiredKeyValuesReady() && eventQueue.length > 0) {
                isRequiredKeyReady = true;
                for (var i = 0; i < eventQueue.length; i++) {
                    //添加必备的公共param的keyValue
                    var finalParams = {}
                    objectAssign(finalParams, eventCommonParams, eventQueue[i].params)
                    eventQueue[i].params = finalParams
                }
                _collect_events(defaultPostData.user, defaultPostData.header, eventQueue);
                eventQueue = []
            } else {
                queueTimer = setTimeout(doExeQueue, 100)
            }
        };

        doExeQueue()
    }


    /**
     * 获取打点服务器的url地址。
     * @return {[string,string]} [xhrURL，imageURL]
     */
    function getServerUrl() {
        var url = XHR_SERVER_URL;
        var imgUrl = IMG_SERVER_URL;

        if (isDebugMod) {
            url = XHR_SERVER_URL_DEV;
            imgUrl = IMG_SERVER_URL_DEV;
        } else {
            if (!isIntranet) {
                var domain = OUT_SERVER_DOMAIN[outServerName];
                url = domain + xhrPath;
                imgUrl = domain + imgPath;
            }
            if (isDisableHttps) {
                url = url.replace(/^https:\/\//, 'http://');
                imgUrl = imgUrl.replace(/^https:\/\//, 'http://');
            }
        }
        return [url, imgUrl];
    }

    function sendToServer(logEvent) {
        sendByXHR(getServerUrl()[0], logEvent)
    }

    //IE9?及以下版本跨域的ajax请求发不出去，需要降级为image请求
    function sendByXHR(url, dataObj, callback) {
        try {
            var XHRHttp = XMLHttpRequest || window.ActiveXObject('Microsoft.XMLHTTP');
            var xhr = new XHRHttp();
            xhr.onreadystatechange = function () {
                if (this.readyState === 4 && !(this.status >= 200 && this.status < 300)) {
                    if (isDebugMod) {
                        console.log(this.status);
                    }
                    sendByImage(dataObj, callback)
                } else {
                    if (this.readyState === 4 && (typeof callback === 'function')) {
                        callback();
                    }
                }
            };
            xhr.open("post", url);
            xhr.setRequestHeader('Content-Type', 'application/json');
            xhr.send(JSON.stringify(dataObj));
        } catch (e) {
            sendByImage(dataObj, callback);
            if (isDebugMod) {
                // console.log('[XHR ERROR] %o', e);
                throw e
            }
        }

    }

    /**
     * 某些情况下会没有Image对象。比如worker里。
     * @param {*} dataObj 
     * @param {*} callback 
     */
    function sendByImage(dataObj, callback) {
        try {
            var url = getServerUrl()[1];
            var str = encodeQueryObj2String(dataObj);

            var img = new Image(1, 1);
            img.onerror = function () {
            };
            img.onload = function () {
                typeof callback === 'function' && callback()
            };
            img.src = url + '?' + str
        } catch (e) {
            if (isDebugMod) {
                // console.log('[XHR ERROR] %o', e);
                throw e
            }
        }
    }

    function sendByBacon(str) {
        //todo
    }

    function getBrowserType() {
        var ret = {}; //用户返回的对象
        var Sys = {};
        var ua = navigator.userAgent.toLowerCase();
        var s;
        if (navigator.appVersion.match(/MSIE 6./i)) {
            ret.type = "IE";
            ret.version = 6;
            return ret;
        } else {
            (s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] :
                (s = ua.match(/firefox\/([\d.]+)/)) ? Sys.firefox = s[1] :
                    (s = ua.match(/chrome\/([\d.]+)/)) ? Sys.chrome = s[1] :
                        (s = ua.match(/opera.([\d.]+)/)) ? Sys.opera = s[1] :
                            (s = ua.match(/version\/([\d.]+).*safari/)) ? Sys.safari = s[1] : 0;
        }

        if (Sys.ie) {
            ret.type = "IE";
            ret.version = Sys.ie;
        } else if (Sys.firefox) {
            ret.type = "Firefox";
            ret.version = Sys.firefox;
        } else if (Sys.chrome) {
            ret.type = "Chrome";
            ret.version = Sys.chrome;
        } else if (Sys.opera) {
            ret.type = "Opera";
            ret.version = Sys.opera;
        } else if (Sys.safari) {
            ret.type = "Safari";
            ret.version = Sys.safari;
        }

        return ret;
    }

    function _setCommonHeaderData() {

        var browserInfo = getBrowserType();
        objectAssign(defaultPostData.header, {
            // //todo 是否需要？暂先下掉。
            // os_name: browserInfo.type,
            // os_version: browserInfo.version,
            //
            // screenResolution: window.screen.width + "x" + window.screen.height,
            // language: navigator.language,
            // network: navigator.connection && navigator.connection.type,

        })
    }

    function encodeQueryObj2String(obj) {
        var string = '';
        for (var key in obj) {
            if (obj.hasOwnProperty(key)) {
                string += '&' + key + '=' + JSON.stringify(obj[key])
            }
        }
        string = string[0] === '&' ? string.slice(1) : string;
        return string
    }

    /**
     * 第一个参数为目标对象，其余参数可以任意多个。
     * @param {object} target 目标对象
     * @return {*}
     */
    function objectAssign(target) {
        for (var i = 1; i < arguments.length; i++) {
            var source = arguments[i];
            for (var key in source) {
                if (Object.prototype.hasOwnProperty.call(source, key)) {
                    target[key] = source[key]
                }
            }
        }
        return target
    }

    function isArray(arg) {
        return Object.prototype.toString.call(arg) === '[object Array]';
    }

    /** =======
     * 初始化
     */


    collect.send = collect;

    collect.setUser = setUser;
    collect.setHeader = setHeader;
    collect.setAppId = setAppId;
    collect.setDebug = setDebug;
    collect.setCaller = setCaller;

    collect.setRequiredKeys = setRequiredKeys;
    collect.setEventCommonParams = setEventCommonParams;
    collect.setHeaderHeaders = setHeaderHeaders;
    collect.setCustomHeader = setCustomHeader;

    collect.disableHttps = disableHttps;
    collect.setIntranetMode = setIntranetMode;

    //非异步情况下，需有个默认值
    window['MarioJSSDKObject'] = window['MarioJSSDKObject'] || 'collectEvent';
    //异步的情况下有这个对象
    if (window[window['MarioJSSDKObject']]) {
        //获取一些浏览器的基础信息
        _setCommonHeaderData();

        //js文件异步加载完成执行后，获取已经设置的配置项。
        requiredKeys = window[window['MarioJSSDKObject']].r || requiredKeys;
        defaultPostData.header.app_id = window[window['MarioJSSDKObject']].id;
        var readyCallback = window[window['MarioJSSDKObject']].ready;
        //执行ready参数。
        if (typeof readyCallback === 'function') {
            readyCallback(collect)
        }

        //添加已经收集的事件
        var queue = window[window['MarioJSSDKObject']].q;
        if (queue) {
            for (var i = 0; i < queue.length; i++) {
                //queue[i]为arguments对象。
                eventQueue.push({
                    event: queue[i][0],
                    params: queue[i][1]
                })
            }
        }
    }

    //发送队列中的事件。
    execQueue();

    return collect

});
